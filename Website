<script>
const LS = {
  mustafa: 'loadstring(game:HttpGet("https://raw.githubusercontent.com/musgamerkeren23-afk/Mustafahub/refs/heads/main/MustafaHub.lua"))()',
  moon: 'loadstring(game:HttpGet("https://raw.githubusercontent.com/musgamerkeren23-afk/Mustafahub/refs/heads/main/MoonHub.lua"))()',
  moonspawn: 'loadstring(game:HttpGet("https://raw.githubusercontent.com/musgamerkeren23-afk/Mustafahub/refs/heads/main/MoonHubSpawner.lua"))()',
  lsgui: 'loadstring(game:HttpGet("https://raw.githubusercontent.com/musgamerkeren23-afk/Mustafahub/refs/heads/main/LoadstringGUI.lua"))()'
}

function showToast(msg, type) {
  const t = document.getElementById('toast')
  t.textContent = msg
  t.className = 'toast ' + type
  t.classList.add('show')
  setTimeout(() => t.classList.remove('show'), 2800)
}

function copyLS(btn, hub) {
  navigator.clipboard.writeText(LS[hub]).then(() => {
    const orig = btn.textContent
    const isMoon = hub.includes('moon')
    btn.textContent = '‚úì Copied!'
    btn.classList.add(isMoon ? 'ok-m' : 'ok-g')
    setTimeout(() => { btn.textContent = orig; btn.classList.remove('ok-g','ok-m') }, 2000)
    showToast('üìã Loadstring disalin!', isMoon ? 'm' : 'g')
  })
}

function dlLS(hub, fname) {
  const blob = new Blob([LS[hub]], {type:'text/plain'})
  const a = document.createElement('a')
  a.href = URL.createObjectURL(blob)
  a.download = fname + '_Loadstring.lua'
  a.click()
  showToast('‚¨áÔ∏è ' + fname + '.lua didownload!', 'd')
}

// SEARCH
const si = document.getElementById('si')
si.addEventListener('input', doFilter)

function clearS() { si.value = ''; doFilter(); }

let curG = 'all'
document.querySelectorAll('.gtab').forEach(tab => {
  tab.addEventListener('click', () => {
    document.querySelectorAll('.gtab').forEach(t => t.classList.remove('on'))
    tab.classList.add('on')
    curG = tab.dataset.g
    doFilter()
  })
})

function doFilter() {
  const q = si.value.toLowerCase().trim()
  const cards = document.querySelectorAll('.sc')
  let vis = 0
  cards.forEach(c => {
    const name = (c.dataset.n || '').toLowerCase()
    const genre = c.dataset.g || ''
    const mQ = !q || name.includes(q)
    const mG = curG === 'all' || genre === curG
    if (mQ && mG) { c.classList.remove('hide'); vis++ }
    else c.classList.add('hide')
  })
  document.getElementById('nr').style.display = vis === 0 ? 'block' : 'none'
}
</script>
</body>
</html>
